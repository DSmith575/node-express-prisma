{
  "name": "realm-api",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test:unit": "jest __tests__/unit",
    "test:integration": "jest __tests__/integration",
    "test:watch": "jest --watch",
    "build": "tsc",
    "start": "node dist/server.js",
    "dev": "nodemon --watch src --ext ts,js,json --exec ts-node -r tsconfig-paths/register src/server.ts",
    "lint": "eslint src --ext .ts,.js",
    "prettier:format": "npx prettier --write . ",
    "docker:run:dev": "docker run --name test-db-dev -e POSTGRES_PASSWORD=HelloWorld123 -p 5432:5432 -d postgres",
    "docker:stop:dev": "docker stop test-db-dev",
    "docker:remove:dev": "docker rm test-db-dev",
    "prisma:generate": "npx prisma generate",
    "prisma:migrate": "npx prisma migrate dev",
    "prisma:reset": "npx prisma migrate reset --force",
    "seed": "ts-node prisma/seed.ts",
    "studio": "npx prisma studio",
    "env:copy": "copy .env.example .env"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.3.0",
    "@typescript-eslint/eslint-plugin": "^8.41.0",
    "@typescript-eslint/parser": "^8.41.0",
    "dotenv": "^17.2.1",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^10.1.8",
    "express": "^5.1.0",
    "install": "^0.13.0",
    "jest": "^30.1.2",
    "nodemon": "^3.1.10",
    "npm": "^11.5.2",
    "prettier": "^3.6.2",
    "prisma": "^6.15.0",
    "ts-jest": "^29.4.1",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.9.2"
  },
  "dependencies": {
    "@prisma/client": "^6.15.0",
    "cors": "^2.8.5",
    "express-rate-limit": "^8.0.1",
    "helmet": "^8.1.0"
  }
}
